{
    "name": "notifyfree/laravel-logger",
    "description": "Laravel logger for NotifyFree service",
    "keywords": [
        "laravel",
        "log",
        "channel",
        "notifyfree",
        "notification",
        "logging"
    ],
    "homepage": "https://github.com/swatchion/notifyfree-laravel-logger",
    "license": "MIT",
    "authors": [
        {
            "name": "NotifyFree Team",
            "email": "dev@notifyfree.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "laravel/framework": "^8.0|^9.0|^10.0|^11.0|^12.0",
        "monolog/monolog": "^2.0|^3.0",
        "guzzlehttp/guzzle": "^6.5|^7.0",
        "guzzlehttp/promises": "^1.4|^2.0"
    },
    "require-dev": {
        "orchestra/testbench": "^9.0",
        "phpunit/phpunit": "^10.0",
        "mockery/mockery": "^1.4"
    },
    "autoload": {
        "psr-4": {
            "NotifyFree\\LaravelLogger\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NotifyFree\\LaravelLogger\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "NotifyFree\\LaravelLogger\\NotifyFreeLoggerServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
